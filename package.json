{
  "name": "@scalable-labs/sentinel",
  "version": "0.1.1",
  "description": "Frontend error monitoring and tracking client with local storage, Microsoft Teams integration, and visual error viewer",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "lefthook install"
  },
  "keywords": [
    "sentinel",
    "error-monitoring",
    "error-tracking",
    "frontend",
    "observability",
    "scalable",
    "monitoring",
    "analytics",
    "fetch",
    "axios",
    "teams",
    "microsoft-teams",
    "error-reporting",
    "browser",
    "indexeddb"
  ],
  "author": "Scalable Labs",
  "license": "MIT",
  "homepage": "https://github.com/scalabledk/sentineljs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scalabledk/sentineljs.git"
  },
  "bugs": {
    "url": "https://github.com/scalabledk/sentineljs/issues"
  },
  "devDependencies": {
    "@evilmartians/lefthook": "^1.13.6",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
